{
  "name": "facturacionelectronicapy-kude",
  "version": "1.0.0",
  "description": "Módulo NodeJs/Java para generar el PDF KUDE en base a un archivo XML",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && copyfiles --up 1 src/**/*.jar dist && copyfiles --up 1 src/**/*.jasper dist && copyfiles --up 1 src/**/*.jar dist",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "",
    "preversion": "",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marcosjara/facturacionelectronicapy-kude.git"
  },
  "bugs": {
    "url": "https://github.com/marcosjara/facturacionelectronicapy-kude/issues"
  },
  "homepage": "https://github.com/marcosjara/facturacionelectronicapy-kude#readme",
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "KUDE",
    "KUDE PDF",
    "Facturación Electrónica",
    "Facturación Electrónica Paraguay",
    "SET"
  ],
  "author": "Marcos Jara",
  "maintainers": [
    "<adrianjara@gmail.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "copyfiles": "^2.4.1",
    "find-java-home": "^1.2.2",
    "node-jasper-report": "^1.0.29"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-mocha": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.4.1",
    "source-map-support": "^0.5.16",
    "supertest": "^6.1.3",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.5"
  }
}
